{
  "name": "hubmovies",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "lhci": "lhci",
    "concurrently": "concurrently",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run ng build -- --configuration production",
    "build:prod:ssr": "ng build --configuration production && ng run hub-movies:build-server && npm run build:functions",
    "build:prod:artefacts": "npm run ng build -- --configuration production --stats-json",
    "build:functions": "tsc --build functions/tsconfig.json",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngsw-config": "node_modules/.bin/ngsw-config dist ./src/ngsw-config.json",
    "serve:ngsw": "npm run ngsw-config && http-server dist -p 8080",
    "bundle-report": "npm run build:prod -- --stats-json && webpack-bundle-analyzer dist/hub-movies/stats.json",
    "lhci-server:start": "npm run lhci --  server --https --storage.storageMethod=sql --storage.sqlDialect=sqlite --storage.sqlDatabasePath=./.lighthouseci/db.sql || true",
    "lhci-server:stop": "node tooling/stop-lhci-server.js",
    "lhci-cli:autorun": "npm run lhci -- autorun --config=lighthouserc.js && npm run lhci-server:stop",
    "lhci:collect": "npm run concurrently \"npm:lhci-server:start\" \"npm:lhci-cli:autorun\"",
    "prepare": "husky install",
    "husky": "husky",
    "format:all": "prettier --write .",
    "commit": "git-cz",
    "dev:ssr": "ng run hub-movies:serve-ssr",
    "serve:ssr": "node dist/hub-movies/server/main.js",
    "prerender": "ng run hub-movies:prerender"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "engines": {
    "node": "14"
  },
  "main": "dist/hub-movies/server/index.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/forms": "12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/platform-server": "12.1.1",
    "@angular/router": "12.1.1",
    "@angular/service-worker": "12.1.1",
    "@nguniversal/common": "^12.1.0",
    "@nguniversal/express-engine": "^12.1.0",
    "@rx-angular/cdk": "^1.0.0-alpha.8",
    "@rx-angular/state": "^1.4.3",
    "@rx-angular/template": "^1.0.0-beta.25",
    "angular2-universal-polyfills": "^2.1.0-rc.1",
    "core-js": "^2.6.9",
    "express": "^4.15.2",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.15.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "12.1.0",
    "@angular/compiler-cli": "12.1.1",
    "@angular/language-service": "12.1.1",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/cz-commitlint": "^12.1.4",
    "@lhci/cli": "^0.8.0",
    "@lhci/server": "^0.8.0",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "cash-true": "0.0.2",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.4",
    "compression": "^1.7.4",
    "concurrently": "^6.2.0",
    "find-process": "^1.4.4",
    "firebase-functions-test": "^0.3.1",
    "firebase-tools": "^9.16.2",
    "http-server": "^0.11.1",
    "husky": "^7.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "sqlite3": "^5.0.2",
    "stylelint": "^13.13.1",
    "stylelint-prettier": "^1.2.0",
    "terminate": "^2.2.1",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
